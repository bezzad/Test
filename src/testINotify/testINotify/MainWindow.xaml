<Window x:Class="testINotify.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:testINotify"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <TextBlock Width="30"/>
            <Button Height="50" Width="200" Content="click " Click="Button_Click"></Button>
            <TextBlock Text="{Binding Texts}" TextAlignment="Center" FontSize="20" Height="50" Width="400"/>
        </DockPanel>
    </Grid>
</Window>
